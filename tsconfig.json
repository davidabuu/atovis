{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/Header/Header.tsx", "ICONS/figmaIcons.js", "src/components/Layout/Layout.js", "src/components/Vendor/ImgLanding.jtsx", "pages/_app.tsx", "src/components/WebLayout/UserWebLayout.tsx"],
  "exclude": ["node_modules"]
}
